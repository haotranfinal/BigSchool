@model IEnumerable<lab3_TranVanHao_2011065143.Models.Course> 
@{
    ViewBag.Title = "Mine";
    Layout = "~/Views/Courses/Create.cshtml";
}

<h2>Mine</h2>
<ul class="course">
    @foreach (var course in Model)
            {
                <li>
                    <div class="date">
                        <div class="month">
                            @course.DateTime.ToString("MM")
                        </div>
                        <div class="day">
                            @course.DateTime.ToString("dd")
                        </div>
                    </div>
                    <div class="details">
                        <span class="lecturer">
                            @course.Lecturer.Name
                        </span>
                        <span class="category">
                            @course.Category.Name
                        </span>
                    </div>
                    <div class="action">b
                        <a href="">Edit</a>
                        <a href="">Delete</a>
                    </div>
                </li>
            }
</ul>